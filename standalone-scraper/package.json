{
  "name": "restomod-scraper",
  "version": "1.0.0",
  "description": "Standalone vehicle scraper for Restomod Central",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "scrape": "tsx src/index.ts scrape",
    "scrape:playwright": "tsx src/scrapers/playwright-scraper.ts",
    "scrape:puppeteer": "tsx src/scrapers/puppeteer-scraper.ts",
    "scrape:cheerio": "tsx src/scrapers/cheerio-scraper.ts",
    "scrape:json": "tsx src/index.ts scrape --output-only",
    "import": "tsx src/index.ts import",
    "import:api": "tsx src/importers/api-importer.ts",
    "import:db": "tsx src/importers/db-importer.ts",
    "test": "vitest",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "scraper",
    "classic-cars",
    "restomod",
    "playwright",
    "automation"
  ],
  "author": "Planet9V",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "playwright": "^1.48.1",
    "puppeteer": "^23.6.1",
    "better-sqlite3": "^11.5.0",
    "dotenv": "^16.4.5",
    "commander": "^12.1.0",
    "winston": "^3.15.0",
    "p-queue": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^22.8.6",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "eslint": "^9.13.0"
  }
}
